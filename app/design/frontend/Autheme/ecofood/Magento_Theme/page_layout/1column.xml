<layout xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_layout.xsd">
    <update handle="empty"/>
    <referenceContainer name="page.wrapper">
        <container name="header.container" as="header_container" label="Page Header Container"  htmlTag="header" htmlClass="page-header" before="-" />

        <container name="eco-slideshow" as="eco-slideshow" label="Page Slideshow Container" htmlTag="section" htmlClass="eco-slideshow clearfix m-b-10" before="main.content"/>

        <container name="eco-free-shipping" as="eco-free-shipping" label="Page Free Shipping" htmlTag="section" htmlClass="eco-free-shipping clearfix" after="eco-slideshow"/>

        <container name="eco-about-us" as="eco-about-us" label="Page Free Shipping" htmlTag="section" htmlClass="eco-about-us-home2 clearfix" after="eco-slideshow"/>

        <container name="eco-our-products-home4" as="eco-our-products-home4" label="Our products home 4" htmlTag="section" htmlClass="section white-background p-t-100 p-b-30 container" after="eco-slideshow"/>

        <container name="eco-our-products" as="eco-our-products" label="Page Our Product" htmlTag="section" htmlClass="m-t-90 m-b-10 clearfix" after="eco-free-shipping"/>

        <container name="eco-massbottom" as="eco-massbottom" label="Page Massbottom Container" htmlTag="section" htmlClass="clearfix" after="eco-deal-of-the-day"/>

        <container name="eco-our-products-home3" as="eco-our-products-home3" label="Page Our Product" htmlTag="section" htmlClass="page-main clearfix" after="eco-deal-of-the-day"/>

        <container name="eco-massbottom1-home3" as="eco-massbottom1-home3" label="Page Our Product" htmlTag="section" htmlClass="clearfix" after="eco-our-products-home3"/>

        <container name="eco-massbottom1" as="eco-massbottom1" label="Page Massbottom1 Container" htmlTag="section" htmlClass="clearfix" after="eco-massbottom"/>

        <container name="eco-massbottom2" as="eco-massbottom2" label="Page Massbottom2 Container" htmlTag="section" htmlClass="clearfix" after="eco-massbottom1"/>

        <container name="eco-massbottom2-home3" as="eco-massbottom2-home3" label="Page Massbottom2 Container" htmlTag="section" htmlClass="clearfix" after="eco-massbottom1-home3"/>

        <container name="eco-massbottom3-home4" as="eco-massbottom3-home4" label="Subscribe homepage 4" htmlTag="section" after="eco-massbottom2"/>

        <container name="eco-about-us-page01" as="eco-about-us-page01" label="About us page 01 " htmlTag="section" before="main.content"/>

        <container name="footer-container" as="footer" before="before.body.end" label="Page Footer Container" htmlTag="footer" htmlClass="page-footer" />
    </referenceContainer>
</layout>
